{
  "type": "file",
  "enabled": true,
  "connection": "s3a://test",
  "config": {
    "fs.s3a.access.key": "minio",
    "fs.s3a.secret.key": "minio123"
  },
  "workspaces": {
    "root": {
      "location": "/",
      "writable": false,
      "defaultInputFormat": null
    }
  },
  "formats": {
    "tsv": {
      "type": "text",
      "extensions": [
        "tsv",
        "log"
      ],
      "extractHeader": true,
      "delimiter": "\u0020",
      "quote": "\u0022"
    }
  }
}
